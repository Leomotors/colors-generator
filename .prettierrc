{
    "bracketSpacing": true,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "semi": true,
    "printWidth": 80,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
        "typescript",
        "jsx"
    ],
    "importOrder": [
        "^\\$styles",
        "^[$]",
        "^@",
        "^next",
        "^react"
    ]
}